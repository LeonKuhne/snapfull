{
  "manifest_version": 3,
  "name": "SnapFull",
  "version": "1.1",
  "description": "Improved desktop video calling UI for Snapchat.",
  "content_scripts": [
    {
      "matches": [
        "*://web.snapchat.com/*"
      ],
      "js": [
        "snapfull.js"
      ],
      "run_at": "document_end"
    }
  ]
}
